  "author": "blipee <dev@blipee.com>",
  "license": "MIT",
  "homepage": "https://github.com/blipee/os-retail-intelligence",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blipee/os-retail-intelligence.git"
  },
  "bugs": {
    "url": "https://github.com/blipee/os-retail-intelligence/issues"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
